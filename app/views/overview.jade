        script.
            function resize(){    
                    $("#chart-area").outerHeight($(window).height()-$("#chart-area").offset().top- Math.abs($("#chart-area").outerHeight(true) - $("#chart-area").outerHeight()));
                }
                $(document).ready(function(){
                    resize();
                    $(window).on("resize", function(){                      
                        resize();
                    });

                   
                });
           
